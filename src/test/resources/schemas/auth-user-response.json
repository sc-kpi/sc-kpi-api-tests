{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Auth User Response",
  "type": "object",
  "required": ["id", "email", "firstName", "lastName", "capabilityTier"],
  "properties": {
    "id": {
      "type": "string",
      "pattern": "^[0-9a-f]{8}-[0-9a-f]{4}-[0-9a-f]{4}-[0-9a-f]{4}-[0-9a-f]{12}$"
    },
    "email": {
      "type": "string"
    },
    "firstName": {
      "type": "string"
    },
    "lastName": {
      "type": "string"
    },
    "capabilityTier": {
      "type": "integer",
      "minimum": 0
    }
  },
  "additionalProperties": false
}
